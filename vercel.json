{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "build",
        "use": "@vercel/static"
      }
    ],
    "routes": [
    {
      "src": "/main/products/(.*)",
      "dest": "/src/app.js"
    },
    {
      "src": "/main/cart/(.*)",
      "dest": "/src/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "build/"
    }
  ]
  }